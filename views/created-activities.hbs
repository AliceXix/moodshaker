
{{#each data}}

    <div>
        <img src="{{this.image}}" alt="image of activity" />
        <p>{{this.title}}</p>

    </div>

<button>
    <a href="/activities/{{this.id}}/details">see more</a>
</button>

<button>
    <a href="/user/{{this.author}}/dashboard">Take me back!</a>
</button>

{{/each}}

